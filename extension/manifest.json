{
  "manifest_version": 2,
  "name": "Focus",
  "version": "1.0",
  "background": {
    "scripts":["./background/background.js"]
  },
  "options_page": "./HTML/options.html",
  "browser_action": {
    "default_popup":"./HTML/popup.html"
  },
  "permissions":[
    "tabs",
    "https://www.google.com/*"
  ]
}