{
  "manifest_version": 2,
  "icons": {
    "128": "./focus 128 x 128.png"
  },
  "name": "Focus",
  "version": "1.0",
  "background": {
    "scripts":["./background/background.js"]
  },
  "options_page": "./popup/options.html",
  "browser_action": {
    "default_popup":"./popup/popup.html"
  },
  "permissions":[
    "tabs",
    "storage",
    "http://localhost/*"
  ]
}